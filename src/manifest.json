{
  "package": "com.cljinchengcp.jcbrocade",
  "name": "锦程测评",
  "versionName": "1.0.0",
  "versionCode": 1,
  "minPlatformVersion": 1121,
  "icon": "/assets/images/logo.png",
  "features": [
    {
      "name": "system.prompt"
    },
    {
      "name": "system.router"
    },
    {
      "name": "system.shortcut"
    },
    {
      "name": "system.fetch"
    },
    {
      "name": "system.device"
    },
    {
      "name": "system.storage"
    },
    {
      "name": "system.webview"
    },
    {
      "name": "system.app"
    }
  ],
  "permissions": [
    {
      "origin": "*"
    }
  ],
  "template/official": "demo-template",
  "config": {
    "logLevel": "off",
    "designWidth": 250
  },
  "router": {
    "entry": "pages/Launch",
    "pages": {
      "pages/Launch": {
        "component": "index"
      },
      "pkg_main/pages/Demo": {
        "component": "index"
      },
      "pkg_main/pages/DemoDetail": {
        "component": "index"
      },
      "pkg_main/pages/ChangshiDetail": {
        "component": "index"
      },
      "pkg_main/pages/Web": {
        "component": "index"
      },
      "pkg_main/pages/CepingIntro": {
        "component": "index"
      },
      "pkg_main/pages/CepingTest": {
        "component": "index"
      }
    }
  },
  "subpackages": [
    {
      "name": "pkg_main",
      "resource": "pkg_main"
    }
  ],
  "display": {
    "titleBarBackgroundColor": "#f2f2f2",
    "titleBarTextColor": "#414141",
    "menu": true,
    "pages": {
      "pages/Launch": {
        "titleBar": false,
        "menu": false
      },
      "pkg_main/pages/Demo": {
        "titleBarText": "锦程测评",
        "titleBar": false,
        "menu": false
      },
      "pkg_main/pages/DemoDetail": {
        "titleBarText": "详情页"
      },
      "pkg_main/pages/ChangshiDetail": {
        "titleBar": false,
        "statusBarImmersive": true
      },
      "pkg_main/pages/Web": {
        "titleBarText": ""
      },
      "pkg_main/pages/CepingIntro": {
        "titleBar": false,
        "statusBarImmersive": true
      },
      "pkg_main/pages/CepingTest": {
        "titleBar": false,
        "statusBarImmersive": true
      }
    }
  },
  "deviceTypeList": ["phone"]
}
